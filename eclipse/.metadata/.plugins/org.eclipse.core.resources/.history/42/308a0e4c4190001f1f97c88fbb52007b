package bolexamen2;

import java.util.Scanner;

public class Ej2_17_18 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num1;
		int num2;
		int num3;
		int cont = 0;
		System.out.println("Introduzca tres nÃºmeros");
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		num3 = sc.nextInt();
		while (num1 >= 0) {
			if (num3 > num2 && num2 > num1) {
				cont = cont + 1;
			}
			
		}
		sc.close();
	}

}
